#include<stdio.h>
int main()
{
    int a[100],b[100],i,j,l;
    printf("Enter the size of Array");
    scanf("%d",&l);
    printf("Enter the Array Elements");
    for(i=0;i<l;i++)
    {
        scanf("%d",&a[i]);
        b[i]=a[i];
    }
    for(j=0;j<l;j++)
    {
        printf("%d",&b[j]);
    }
    return 0;
}